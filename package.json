{
  "name": "lightkey-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-react-app": "^0.5.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "flow-bin": "^0.56.0",
    "husky": "^0.14.3",
    "intl": "^1.2.5",
    "json-server": "^0.12.0",
    "lint-staged": "^4.2.3",
    "mdi-react": "^2.0.46",
    "moment": "^2.19.2",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.7.4",
    "rc-calendar": "^9.3.0",
    "rc-time-picker": "^3.2.1",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.0.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^1.0.14",
    "recharts": "^1.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.1.2",
    "redux-thunk": "^2.2.0",
    "route-parser": "0.0.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js dev-server",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "flow": "flow",
    "production-server": "json-server --static ./build db.json",
    "dev-server": "json-server -p 8081 db.json"
  },
  "devDependencies": {
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "prettier": "^1.7.4"
  },
  "proxy": {
    "/lightkey/api": {
      "target": "http://127.0.0.1:8099"
    }
  }
}
